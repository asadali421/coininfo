
{{#if error_message}}
    <div class="alert alert-danger">
        {{error_message}}
    </div>
{{/if}}


{{#if error}}
    <div class="alert alert-dark">
        {{error}}
    </div>
{{/if}}



{{#if success_message }}
    <div class="alert alert-success">
        {{success_message}}
    </div>
{{/if}}

{{#if success}}
    <div class="alert alert-success">
        {{success}}
    </div>
{{/if}}
